name: |-
  ziwi签到
desc: |-
  微信公众号搜索ZIWI+小程序 自动完成签到任务

http:

  mitm:
    - "ziwixcx.escase.cn"

  script:
# 获取Cookie
    - match: ^https:\/\/ziwixcx\.escase\.cn\/json-rpc\?__method=GetTaskList
      name: "ziwi签到_6"
      type: request

cron:
  script:
# （默认晚上12点 早上5点 中午12点 执行，如需更改请自行修改corn表达式）
    - name: "ziwi签到_6"
      cron: "0 0,5,12 * * *"

script-providers:
  "ziwi签到_6":
    url: https://raw.githubusercontent.com/anyehttp/quantumult-x/main/task/ziwi+.js
    interval: 86400